PROGRAM fibonacci
BEGIN

	STRING dummy := "abcde";

	INT i,result; 


	FUNCTION INT F (INT n)
	BEGIN

		IF (n > 2)
		THEN
			RETURN F(n-1)+F(n-2);
		ELSE
			IF (n = 0)   --This is a comment
			THEN
				RETURN 0;
			ELSE
				IF (n = 1)
 				THEN
					RETURN 1;
				ENDIF
	
				IF (n = 2)
				THEN
					RETURN 1;
				ENDIF
			ENDIF
		ENDIF
	END
	
	
	FUNCTION VOID main ()
	BEGIN
		
    INT i, end, result;
		READ(end);

	i := 0;
 	REPEAT
		result := F(i);
		WRITE (i);
		WRITE (result);
		i := i + 1;	
	UNTIL (i = end);


	END

END	
